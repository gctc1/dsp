clc
clear all
close all
x=input('ENTER THE SEQUENCE')
n=input('ENTER THE time insatnces')
l=input('ENTER THE interpolation factor')
j=1
for i=2:length(n)
    z(j)=x(i)
    j=j+l
end
d=input('ENTER THE decimation factor')

y=z(1:d:length(z)-1)
n1=1:length(z)
n11=1:length(y)
subplot(3,1,1)
stem(n,x);
subplot(3,1,2);
stem(n1,z);
subplot(3,1,3)
stem(n11,y)
